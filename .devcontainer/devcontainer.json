        "source=${localWorkspaceFolder}/.devcontainer,target=/arbitrage_algorithm/.devcontainer,type=bind,consistency=cached",
        "source=${localWorkspaceFolder}/Dockerfile,target=/arbitrage_algorithm/Dockerfile,type=bind,consistency=cached"
    ],
    "workspaceFolder": "/arbitrage_algorithm",
    "customizations": {
        "vscode": {
